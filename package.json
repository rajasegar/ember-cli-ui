    "express-ws": "^4.0.0",
    "filesize": "^6.1.0",
    "get-port": "^5.1.1",
    "node-pty": "^0.9.0",
    "npm-check-updates": "^7.1.1",
    "semver": "^7.3.2",
    "walk-sync": "^2.2.0",
    "xterm": "^4.8.1",
    "xterm-addon-attach": "^0.6.0",
    "xterm-addon-fit": "^0.4.0",
    "xterm-addon-web-links": "^0.4.0"
  },
  "ember-addon": {}
}
